# GitHub Pages URL (if using GitHub Pages)
# Example: https://yourusername.github.io/mcp-human
GITHUB_PAGES_URL=

# Form server URL (if not using GitHub Pages)
# Default: http://localhost:3000
FORM_SERVER_URL=http://localhost:3000

# Optional callback URL for form submissions
# Example: https://your-server.com/api/mturk-callback
CALLBACK_URL=

# MTurk configuration
# Use sandbox by default (set to "false" for production)
MTURK_SANDBOX=true
AWS_REGION=us-east-1

# AWS credentials (required)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=